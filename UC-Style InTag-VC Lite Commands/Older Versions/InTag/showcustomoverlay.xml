<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<menus>
  <!-- global commands to bring up the show numbers overlay -->
  <menu name="ShowOverlay" type="normal">
      <requires>
    </requires>
     <commands>
      <command type="vp-script">
        <name>
          <![CDATA[<createcanvas1>]]>
        </name>
        <actions>
          <customoverlay action="create" />
        </actions>
      </command>
      <command type="vp-script">
        <name>
          <![CDATA[<showcanvas1>]]>
        </name>
        <actions>
          <customoverlay action="display" />
        </actions>
      </command>
      <command type="vp-script">
        <name>
          <![CDATA[<showcanvas1> <1to50abc>]]>
        </name>
        <actions>
          <customoverlay action="display" number="_value2" />
        </actions>
      </command>
      <command type="vp-script">
        <name>
          <![CDATA[open touch <1to50abc>]]>
        </name>
        <actions>
          <customoverlay action="display" />
          <wait ms="1000"/>
      <overlaymouseaction action="click" numbers="_value1" />
        </actions>
      </command>
      <command type="vp-script">
        <name>
          <![CDATA[open follow <1to50abc>]]>
        </name>
        <actions>
          <customoverlay action="display" />
          <wait ms="1000"/>
      <overlaymouseaction action="click" numbers="_value1" stayopen="true" />
        </actions>
      </command>
    </commands>
<lists>
