        <actions>
          <wait ms="500"/>
          <wait ms="500"/>
          <wait ms="500"/>
          <wait ms="500"/>
          <place Monitor ="left" align="topright" resizeRatioX="1/1" resizeRatioY="1/1" />
          <wait ms="300"/>
            <numbers action="display" />
        </actions>
      </command>
