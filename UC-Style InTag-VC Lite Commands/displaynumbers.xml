<menus>
  <!-- global commands to bring up the show numbers overlay FOR Chrome -->
  <menu name="ChromeShowNumbers" type="normal">
    <requires>
      <activeApp exe="chrome"></activeApp>
    </requires>
    <commands>

	<command type="vp-script">
        <name>
          <![CDATA[<chromenumbers1>]]>
        </name>
        <actions>
          <customoverlay action="close" />
          <wait ms="500"/>
            <numbers action="display" />
        </actions>
      </command>

	  <command type="vp-script">
        <name>
          <![CDATA[create intag report]]>
        </name>
        <actions>
          <numbers action="display" record="true"/>
        </actions>
      </command>
        <command type="vp-script">
          <name>
            <![CDATA[intag menu]]>
          </name>
          <actions>
            <numbers action="display" controller="desktop" />
          </actions>
        </command>

		<command type="vp-script">
   <name>
    <![CDATA[intag startt menu]]>
  </name>
  <actions>
    <numbers action="display" target="startmenu"/>
  </actions>
</command>

<command type="vp-script">
   <name>
    <![CDATA[intag start menu]]>
  </name>
  <actions>
    <numbers action="display" target="startmenu"/>
  </actions>
</command>

<command type="vp-script">
   <name>
    <![CDATA[intag desktop]]>
  </name>
  <actions>
   <mimic phrase="show desktop" />
      <wait ms="100"/>
   <mimic phrase="intag window" />
  </actions>

  </command>
        <command type="vp-script">
          <name>
            <![CDATA[close numbers]]>
          </name>
          <actions>
          <numbers action="close" />
          </actions>
        </command>
      </commands>
      <lists>
        <List>
          <name>chromenumbers1</name>
          <items>
            <item>show numbers</item>
            <item>intag that</item>
            <item>intag it</item>
            <item>show intags</item>
            <item>intag window</item>
            <item>show Chrome numbers</item>
           <item>display numbers</item>
          </items>
        </List>
      </lists>
    </menu>
