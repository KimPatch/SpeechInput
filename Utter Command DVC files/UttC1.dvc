		COMMAND "<UCList> <ExportImportMergeFileHelpOptions>"{
			SCRIPT {
				' OpenListTab at 115
				DllCall "UttC.dll", 115, "File"
				Wait 1000
				IF _arg2 = "Help" THEN 
				SendKeys "{Alt+h}"
				ELSE
				IF _arg2 = "Options" THEN 
				SendKeys "{Alt+t}"
				SendKeys "t"
				SendKeys "{Enter}"
				ELSE
				IF _arg2 = "Options Thin" THEN 
				SendKeys "{Alt+t}"
				SendKeys "t"
				SendKeys "{Enter}"
				Wait 300
				SendKeys "{Alt+t}"
				Wait 500
				SendKeys "{Enter}"
				ELSE
				IF _arg2 = "Options List" THEN 
				SendKeys "{Alt+t}"
				SendKeys "t"
				SendKeys "{Enter}"
				Wait 300
				SendKeys "{Alt+l}"
				Wait 500
				SendKeys "{Enter}"
				ELSE
				SendKeys "{Alt+f}"
				IF _arg2 = "Export" THEN SendKeys "e"
				IF _arg2 = "Import" THEN SendKeys "i"
				IF _arg2 = "Merge" THEN SendKeys "m"
				Wait 100
				IF _arg2 = "Export" THEN
				SendKeys "{Home}{Shift+Right}"
				ELSE
				SendKeys "{Shift+Tab}{Ctrl+End}"
				END IF
				END IF
				END IF
				END IF
				END IF
			}
		}
