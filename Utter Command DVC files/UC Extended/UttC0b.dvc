	STATE "Global Commands" GLOBAL {
 
#Tab 
		COMMAND "<Tab List> <1-10> Tab" {
			SCRIPT {
				v$ = DllCall$("UttC.dll", "ParseListItem", "D|V|" + _arg1, 512)
				' At this point v$ is the PATH part of the list item
				SendKeys v$
				Wait 10
				SendKeys "{Tab " + _arg2 + "}"
			}
		} 
		COMMAND "<Tab List> Stay" {
			SCRIPT {
				v$ = DllCall$("UttC.dll", "ParseListItem", "D|V|" + _arg1, 512)
				SendKeys v$
