				v$ = DllCall$("UttC.dll", "ParseListItem", "D|V|" + _arg2, 512)
				SendKeys v$
				IF _arg1 = "Spaces" THEN SendKeys " "
			}
		}
		LIST "Punct" {
			"Comma"  "Slash"  "Period"  "Space"  "Spaces"  "No Space"
		}
 
