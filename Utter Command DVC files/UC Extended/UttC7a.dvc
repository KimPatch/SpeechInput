		COMMAND "<1-20> <GLLWSP> <BackForward>" {
			SCRIPT {
					IF _arg2 = "Letter" THEN  _arg2 = "Letters"
					IF _arg2 = "Word" THEN  _arg2 = "Words"
					IF _arg2 = "Line" THEN  _arg2 = "Lines"
					IF _arg2 = "Graph" THEN  _arg2 = "Graphs"
					IF _arg2 = "Graph Space" THEN  _arg2 = "Graph Spaces"
					IF _arg2 = "Page" THEN  _arg2 = "Pages"
