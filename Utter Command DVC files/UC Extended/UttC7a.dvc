		LIST "GLLWSP" {
			"Letter"  "Word"  "Line"
			"Graph"  "Graph Space"  "Page"
		}
		COMMAND "<GLLWBsAsUS> <1-20> <BackForward>" {
			SCRIPT {
				' ThisCommand at 91
				DllCall "UttC.dll",91, _arg1
				SendKeys "{Ctrl+x}"
				IF _arg3 = "Back" THEN 
					IF _arg1 = "Letter" XOR _arg1 = "Word" XOR _arg1 = "Line" XOR _arg1 = "Graph" THEN 
						p$ = _arg2 + "|" + _arg1 + "s|" + "Before"
					End If
					IF _arg1 = "Befores" XOR _arg1 = "Afters" THEN
						p$ = _arg2 + "|" + "Words" + "|" + "Before"
					End If
					IF _arg1 = "Lefts" XOR _arg1 = "Rights" THEN 
						p$ = _arg2 + "|" + "Letters" + "|" + "Before"
					End If
					IF _arg1 = "Line Up" THEN
						p$ = _arg2 + "|" + "Lines" + "|" + "Before"
					End If
					IF _arg1 = "Graph Up" THEN
						p$ = _arg2 + "|" + "Graph Ups" + "|" + "Before"
					End If
					IF _arg1 = "Graph Space" THEN
						p$ = _arg2 + "|" + "Graph Spaces" + "|" + "Before"
					End If
					IF _arg1 = "Graph Up Space" THEN
						p$ = _arg2 + "|" + "Graph Up Spaces" + "|" + "Before"
					End If
				' GLLWUDUs at 47
				DllCall "UttC.dll", 47, p$
				END IF
				IF _arg3 = "Forward" THEN 
					IF _arg1 = "Letter" XOR _arg1 = "Word" XOR _arg1 = "Line" XOR _arg1 = "Graph" THEN
						p$ = _arg2 + "|" + _arg1 + "s|" + "After"
					End If
					IF _arg1 = "Befores" XOR _arg1 = "Afters" THEN
						p$ = _arg2 + "|" + "Words" + "|" + "After"
					End If
					IF _arg1 = "Lefts" XOR _arg1 = "Rights" THEN 
						p$ = _arg2 + "|" + "Letters" + "|" + "After"
					End If
					IF _arg1 = "Line Up" THEN
						p$ = _arg2 + "|" + "Lines" + "|" + "After"
					End If
					IF _arg1 = "Graph Up" THEN
						p$ = _arg2 + "|" + "Graph Ups" + "|" + "After"
					End If
					IF _arg1 = "Graph Space" THEN
						p$ = _arg2 + "|" + "Graph Spaces" + "|" + "After"
					End If
					IF _arg1 = "Graph Up Space" THEN
						p$ = _arg2 + "|" + "Graph Up Spaces" + "|" + "After"
					End If
				' GLLWUDUs at 47
				DllCall "UttC.dll", 47, p$
				END IF
				Wait 500
				SendKeys "{Ctrl+v}"
				IF _arg1 = "Letter" THEN SendKeys "{Shift+Left}"
				IF _arg1 = "Word" XOR _arg1 = "Befores" XOR _arg1 = "Afters" THEN SendKeys "{Ctrl+Shift+Left}"
				IF _arg1 = "Line" XOR _arg1 = "Line Up" THEN SendKeys "{End}{Shift+Home}"
				IF _arg1 = "Graph" XOR _arg1 = "Graph Up" THEN SendKeys "{Shift+Ctrl+Up}"
				IF _arg1 = "Graph Space" XOR _arg1 = "Graph Up Space" THEN SendKeys "{Shift+Ctrl+Up 2}"
			}
		}
		LIST "GLLWBsAsUS" {
			"Letter"
			"Word"  "Befores"  "Afters"
			"Line"  "Line Up"
			"Graph"  "Graph Up"  "Graph Space"  "Graph Space Up"
		}
		COMMAND "1 <GLLWUDUs> <1-20> <BackForward>" {
			SCRIPT {
				p$ = "1" + "|" + _arg1
				' GLLWDUABUSs at 46
				DllCall "UttC.dll", 46, p$
				p$ = _arg3 + "|" + _arg2 + "|" + _arg1
				' backforward1to20GLLWSPs at 9
				DllCall "UttC.dll", 9, p$
				IF _arg1 = "Letters" XOR _arg1 = "Rights" XOR _arg1 = "Lefts" THEN SendKeys "{Shift+Left}"				
				IF _arg1 = "Words" XOR _arg1 = "Befores" XOR _arg1 = "Afters" THEN SendKeys "{Ctrl+Shift+Left}"
				IF _arg1 = "Lines" XOR _arg1 = "Line Ups" THEN SendKeys "{End}{Shift+Home}"
				IF _arg1 = "Ups" XOR _arg1 = "Downs" THEN SendKeys "{Shift+Up}"
				IF _arg1 = "Graphs" XOR _arg1 = "Graph Ups" THEN SendKeys "{Shift+Ctrl+Up}"
				IF _arg1 = "Graph Spaces" XOR _arg1 = "Graph Space Ups" THEN SendKeys "{Shift+Ctrl+Up 2}"								
			}
		}
