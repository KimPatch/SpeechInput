				IF _arg2 = "Lines" XOR _arg2 = "Line Ups" THEN SendKeys "{Shift+Up " + _arg1 + "}"
				IF _arg2 = "Ups" XOR _arg2 = "Downs" THEN SendKeys "{Shift+Up " + _arg1 + "}"
				IF _arg2 = "Graphs" XOR _arg2 = "Graph Ups" THEN SendKeys "{Shift+Ctrl+Up " + _arg1 + "}"
				IF _arg2 = "Graph Spaces" XOR _arg2 = "Graph Space Ups" THEN SendKeys "{Shift+Ctrl+Up " + _arg1 + "}{Shift+Ctrl+Up " + _arg1 + "}"								
			}
		}
