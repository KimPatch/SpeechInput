		COMMAND "<1-20> <GLLWDUABUSs> <TouchTTwiceTRight>" {
			SCRIPT {
				IF _arg2 = "Graph Space Ups" THEN SendKeys "{Ctrl+Up}"
				IF _arg2 = "Befores" THEN
				SendKeys "{Shift+Ctrl+Left " + _arg1 + "}" 
				ELSE
				p$ = _arg1 + "|" + _arg2
				' GLLWDUABUSs at 46
				DllCall "UttC.dll", 46, p$
				END IF
				IF _arg3 = "Touch" THEN ButtonClick
				IF _arg3 = "Touch Right" THEN ButtonClick 2,1
				IF _arg3 = "Touch Twice" THEN ButtonClick 1, 2
 
			}
		}
