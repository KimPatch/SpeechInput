		COMMAND "<TouchTTwice> <1-20> <GLLWDUABUSs> <CutCopyPasteDeleteBoldItalicTouch>" {
			SCRIPT {
				IF _arg1 = "Touch" THEN ButtonClick
				IF _arg1 = "Touch Twice" THEN ButtonClick 1, 2
				p$ = _arg2 + "|" + _arg3
				' GLLWDUABUSs at 46
				DllCall "UttC.dll", 46, p$
				IF _arg4 = "Touch" THEN
				Wait 1000
				ButtonClick
				ELSE
				Wait 200
				' ThisCommand at 91
				DllCall "UttC.dll",91, _arg4
				END IF
			}
		}
