#4a. Clicking including holding
 
		COMMAND "<TouchAllTouch>" {
			SCRIPT {
				p$ = _arg1
				' Touch at 92
				DllCall "UttC.dll", 92, p$
			}
		}
		COMMAND "<TouchTTwice> Times <1-20>" {
 			SCRIPT {
				loop& = Val(_arg2)
				while ( loop& )
				p$ = _arg1
				' Touch at 92
				DllCall "UttC.dll",92,p$
				Wait 10
				loop& = loop& - 1
				wend
			}
		}
		COMMAND "<1-20> <TouchTTwice>" {
 			SCRIPT {
				loop& = Val(_arg1)
				while ( loop& )
				p$ = _arg2
				' Touch at 92
				DllCall "UttC.dll",92,p$
				Wait 10
				loop& = loop& - 1
				wend
			}
		}
		COMMAND "<1-20> <TouchTTwice> Wait" {
 			SCRIPT {
				loop& = Val(_arg1)
				while ( loop& )
				p$ = _arg2
				' Touch at 92
				DllCall "UttC.dll",92,p$
				Wait 1000
				loop& = loop& - 1
				wend
			}
		}
		COMMAND "<1-20> <TouchTTwice> Wait <1-10>" {
 			SCRIPT {
				loop& = Val(_arg1)
				while ( loop& )
				p$ = _arg2
				' Touch at 92
				DllCall "UttC.dll",92,p$
				Wait Val(_arg3)*1000
				loop& = loop& - 1
				wend
			}
		}
		COMMAND "<TouchHoldTRelease>" {
			SCRIPT {
				p$ = _arg1
				' Touch at 92
				DllCall "UttC.dll", 92, p$
				TTSPlayString "" + _arg1 + ""
			}
		}		
		COMMAND "<CutCopyPasteDeleteBoldItalic> <TouchHoldTRelease>" {
			SCRIPT {
				' ThisCommand at 91
				DllCall "UttC.dll", 91, _arg1
				p$ = _arg2
				' Touch at 92
				DllCall "UttC.dll", 92, p$
				TTSPlayString "" + _arg2 + ""
			}
		}
		COMMAND "<TouchHoldTRelease> <CutCopyPasteDeleteBoldItalic>" {
			SCRIPT {
				p$ = _arg1
				' Touch at 92
				DllCall "UttC.dll", 92, p$
				TTSPlayString "" + _arg1 + ""
				' ThisCommand at 91
				DllCall "UttC.dll", 91, _arg2
			}
		}
		COMMAND "<TouchHoldTRelease> <1-3AltUnderCtrlSh> <AlphChar0-9>" {
			SCRIPT {
				p$ = _arg1
				' Touch at 92
				DllCall "UttC.dll", 92, p$
				TTSPlayString "" + _arg1 + ""
				Wait 10
				HeardWord _arg2, _arg3
			}
		}
		COMMAND "<1-3AltUnderCtrlSh> <AlphChar0-9> <TouchHoldTRelease>" {
			SCRIPT {
				HeardWord _arg1, _arg2
				p$ = _arg3
				' Touch at 92
				DllCall "UttC.dll", 92, p$
				TTSPlayString "" + _arg3 + ""
			}
		}
		COMMAND "<TouchHoldTRelease> Letter <Alph>" {
			SCRIPT {
				
				p$ = _arg1
				' Touch at 92
				DllCall "UttC.dll", 92, p$
				TTSPlayString "" + _arg1 + ""
				Wait 10
				SendKeys MID$(_arg2,0,1)
			}
		}
		COMMAND "Letter <Alph> <TouchHoldTRelease>" {
			SCRIPT {
				
				SendKeys MID$(_arg1,0,1)
				p$ = _arg2
				' Touch at 92
				DllCall "UttC.dll", 92, p$
				TTSPlayString "" + _arg2 + ""
			}
		}
		LIST "Alph" {	  
			"a" "b" "c" "d" "e" "f" "g" "h" "i" "j" "k" "l" "m"
			"n" "o" "p" "q" "r" "s" "t" "u" "v" "w" "x" "y" "z" 
			"alpha"  "bravo"  "charlie"  "delta"  "echo"  "foxtrot"  "golf"  "hotel"  "india"  "juliett"  "kilo"  "lima"  "mike"  "november"  "oscar"  "papa"  "quebec"  "romeo"  "sierra"  "tango"  "uniform"  "victor"  "whiskey"  "xray"  "yankee"  "zulu"  
		}
		LIST "TouchHoldTRelease" {
			"Touch Hold"  "Touch Release"
		}
 
#b. Clicking and and hitting keys
 
		COMMAND "<TouchTTwice> <1-20> <EnterSpace>" {
			SCRIPT {
				p$ = _arg1
				' Touch at 92
				DllCall "UttC.dll", 92, p$
				Wait 200 
				SendKeys "{" + _arg3 + " " + _arg2 + "}"
			}
		}
		LIST "EnterSpace" {
			"Enter"  "Space"
		}
		COMMAND "<TouchTTwiceTRight> Function <1-12>" {
			SCRIPT {
				IF _arg1 = "Touch" THEN ButtonClick
				IF _arg1 = "Touch Twice" THEN ButtonClick 1, 2
				IF _arg1 = "Touch Right" THEN ButtonClick 2,1
				Wait 500
				SendKeys "{F" + _arg2 + "}"
			}
		}
		COMMAND "Function <1-12> <TouchTTwiceTRight>" {
			SCRIPT {
				SendKeys "{F" + _arg1 + "}"
				Wait 500	
				IF _arg2 = "Touch" THEN ButtonClick
				IF _arg2 = "Touch Twice" THEN ButtonClick 1, 2
				IF _arg2 = "Touch Right" THEN ButtonClick 2,1
			}
		}
		COMMAND "<EnterEscapeSpace> <TouchTTwiceTRight>" {
			SCRIPT {
				IF _arg1 = "Escape" THEN  _arg1 = "Esc"
				SendKeys "{" + _arg1 + "}"
				Wait 500	
				IF _arg2 = "Touch" THEN ButtonClick
				IF _arg2 = "Touch Twice" THEN ButtonClick 1, 2
				IF _arg2 = "Touch Right" THEN ButtonClick 2,1
			}
		}
		LIST "EnterEscapeSpace" {
			"Enter"  "Escape"  "Space"
		}
		COMMAND "<TouchTTwice> <1-40> <KeyBkDelEnterSpaceTabShiftTEscapeClosePgDnPgUp>" {
			SCRIPT {
				IF _arg1 = "Touch" THEN ButtonClick
				IF _arg1 = "Touch Twice" THEN ButtonClick 1, 2
				IF _arg3 = "Escape" THEN  _arg3 = "Esc"
				IF _arg3 = "Delete" THEN  _arg3 = "Del"
				IF _arg3 = "Shift Tab" THEN  _arg3 = "Shift+Tab"
				IF _arg3 = "Space" THEN  _arg3 = " "
				IF _arg3 = "Close" THEN  _arg3 = "Esc"
				IF _arg3 = "Page Up" THEN  _arg3 = "PgUp"
				IF _arg3 = "Page Down" THEN  _arg3 = "PgDn"
				loop& = Val(_arg2)
				while ( loop& )
				SendKeys "{" + _arg3 + "}"
				loop& = loop& - 1
				wend
			}
		}
		LIST "KeyBkDelEnterSpaceTabShiftTEscapeClosePgDnPgUp" {
 			"Page Up"  "Page Down" 
			"Delete"  "Backspace"  "Space"  "Enter"  "Tab" "Shift Tab"
			"Escape"  "Close"  
		}
		COMMAND "<TouchTTwice> <1-3AltUnderCtrlSh> <AlphChar0-9> <1-3AltUnderCtrlSh> <AlphChar0-9>" {
			SCRIPT {
				IF _arg1 = "Touch" THEN ButtonClick
				IF _arg1 = "Touch Twice" THEN ButtonClick 1, 2
				HeardWord _arg2, _arg3, _arg4, _arg5
			}
		}
		COMMAND "<1-10> Enter <TouchTTwice> Under <LeftRight>" {
			SCRIPT {
				SendKeys "{Enter " + _arg1 + "}"
				IF _arg2 = "Touch" THEN ButtonClick
				IF _arg2 = "Touch Twice" THEN ButtonClick 1, 2
				IF _arg3 = "Left" THEN SendKeys "{Alt+Left}"
				IF _arg3 = "Right" THEN SendKeys "{Alt+Right}"
			}
		}
		LIST "LeftRight" {
			"Left"  "Right"	
		}
