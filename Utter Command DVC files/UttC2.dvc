		COMMAND "<CutCopyAllWLGCopy> To <EudoraOutlookExpressThunderbird> <NN>" {
			SCRIPT {
				IF _arg1 = "Copy" THEN SendKeys "{Ctrl+c}"
				IF _arg1 = "All Copy" THEN SendKeys "{Ctrl+Home}{Shift+Ctrl+End}{Ctrl+c}"
				IF _arg1 = "All Copy" THEN SendKeys "{Ctrl+a}{Ctrl+c}"
				IF _arg1 = "Word Copy" THEN SendKeys "{Ctrl+Left}{Shift+Ctrl+Right}{Ctrl+c}"
				IF _arg1 = "Line Copy" THEN SendKeys "{Home}{Shift+End}{Ctrl+c}"
				IF _arg1 = "Graph Copy" THEN SendKeys "{Down}{Ctrl+Up}{Shift+Ctrl+Down}{Ctrl+c}"
				IF _arg2 = "Express" THEN _arg2 = "msimn"
				AppBringUp "" + _arg2 + ""
				Wait 3000
				IF _arg2 = "Thunderbird" THEN 
				IF _arg1 = "Outlook" THEN 
				param$ = "99 99"
				' MoveArrowByPercentagesLocal at 57
				DllCall "UttC.dll", 57, param$
				ButtonClick
				SendKeys "{Ctrl+m}"
				ELSE
				SendKeys "{Ctrl+n}"
				END IF
				Wait 100
				SendKeys "" + _arg3 + ""
				IF _arg2 = "Eudora" THEN SendKeys "{Alt+a}"
				IF _arg2 = "Thunderbird" THEN SendKeys "{Alt+s}"
				IF _arg2 = "msimn" THEN SendKeys "{Shift+Tab}"
				IF _arg2 = "Outlook" THEN 
				SetMousePosition 1, 500, 700
				ButtonClick
				SendKeys "{Ctrl+Home}"
				END IF
				IF _arg2 = "Eudora" XOR _arg2 = "Thunderbird" THEN SendKeys "{Tab}"
				Wait 300
				SendKeys "{Enter 2}{Ctrl+v}{Enter}{Ctrl+Home}"
				Wait 300
				SendKeys "Hi " + _arg3 + ","
				IF _arg2 = "Eudora" THEN 
				SendKeys "{Alt+u}"
				ELSE
				IF _arg2 = "Thunderbird" THEN 
				SendKeys "{Alt+s}"
				ELSE
				SendKeys "{Shift+Tab}"
				END IF
				END IF
				END IF
			}
		}
 
