#8a Sending and checking email
		COMMAND "<EudoraOutlookExpressThunderbird> Message" {
			SCRIPT {
				IF _arg1 = "Mail" THEN 
					' DefaultMail at 14
					DllCall "UttC.dll",14, _arg1
				ELSE
				IF _arg1 = "Express" THEN _arg1 = "msimn"
				AppBringUp "" + _arg1 + ""
				Wait 3000
				IF _arg1 = "Thunderbird" THEN
				SendKeys "{Ctrl+m}"
				ELSE
				IF _arg1 = "Outlook" THEN
				param$ = "99 99"
				' MoveArrowByPercentagesLocal at 57
				DllCall "UttC.dll", 57, param$
				ButtonClick
				SendKeys "{Shift+Ctrl+m}"
				ELSE
				SendKeys "{Ctrl+n}"				
				END IF
				END IF
				END IF
			}
		}
		COMMAND "<EudoraOutlookExpressThunderbird> Message <Alph>" {
