#8b Sending to Nicknames (NN)
		COMMAND "Testing <NN>" {
			SCRIPT {
				n$ = DllCall$("UttC.dll", "ParseListItem", "N|N|" + _arg1, 512)
				v$ = DllCall$("UttC.dll", "ParseListItem", "D|V|" + _arg1, 512)
				Wait 100
				SendKeys v$
				Wait 100
				SendKeys "Hi " + n$ + ", {Enter 2}"
			}
		}
 
		COMMAND "<EudoraOutlookExpressThunderbird> <NN>" {
			SCRIPT {
				n$ = DllCall$("UttC.dll", "ParseListItem", "N|N|" + _arg2, 512)
				v$ = DllCall$("UttC.dll", "ParseListItem", "D|V|" + _arg2, 512)
				IF _arg1 = "Express" THEN _arg1 = "msimn"
				AppBringUp "" + _arg1 + ""
				IF _arg1 = "Outlook" THEN
				Wait 2000
				END IF
				Wait 300
			    ' WaitForWindowByTitleRE at 122
			    DllCall "UttC.dll", 122, "7|Eudora|Express|Thunderbird|Outlook"
				Wait 300
				IF _arg1 = "Outlook" THEN 
				param$ = "98 98"
				' MoveArrowByPercentagesLocal at 57
				DllCall "UttC.dll", 57, param$
				ButtonClick
				Wait 50
				SendKeys "{Shift+Ctrl+m}"
				ELSE
				IF _arg1 = "Thunderbird" THEN 
				SendKeys "{Ctrl+m}"
				ELSE
				IF _arg1 = "msimn" THEN 
				SendKeys "{Shift+Tab}"
				SendKeys "{Ctrl+n}"
				ELSE
				SendKeys "{Ctrl+n}"
				END IF
				END IF
				END IF
				Wait 500
				SendKeys v$
				Wait 100
				IF _arg1 = "Thunderbird" THEN 
				Wait 500
				SendKeys "{Enter}"
				Wait 500
				SendKeys "{Alt+s}{Tab}"
				ELSE
				IF _arg1 = "Eudora" THEN 
				Wait 100
				SendKeys "{Alt+a}{Tab}"
				ELSE
				IF _arg1 = "msimn" THEN 
				Wait 500
				SendKeys "{Shift+Tab}"
				ELSE
				Wait 500
				SendKeys "{Enter}"
				SendKeys "{Tab 4}"
				SendKeys "{Home}"
				END IF
				END IF
				END IF
				Wait 500
				SendKeys "Hi " + n$ + ", {Enter 2}"
				IF _arg1 = "Thunderbird" THEN 
				SendKeys "{Alt+s}"
				ELSE
				IF _arg1 = "Eudora" THEN 
				SendKeys "{Alt+u}"
				ELSE
				SendKeys "{Shift+Tab}"
				END IF
				END IF
			}
		}
		COMMAND "<EudoraOutlookExpressThunderbird> <NN> <NoDearHiPlain>" {
			SCRIPT {
				IF _arg1 = "Express" THEN _arg1 = "msimn"
				IF _arg1 = "Outlook" THEN
				p$ = "50 70"
				' MoveArrowByPercentages at 56
				DllCall "UttC.dll", 56, p$
				END IF
				AppBringUp "" + _arg1 + ""
				IF _arg1 = "Outlook" THEN 
				Wait 2000
				END IF
				Wait 300
			    ' WaitForWindowByTitleRE at 122
			    DllCall "UttC.dll", 122, "7|Eudora|Express|Thunderbird|Outlook"
				Wait 300
				IF _arg1 = "Outlook" THEN 
				SendKeys "{Shift+Ctrl+m}"
				ELSE
				IF _arg1 = "Thunderbird" THEN 
				SendKeys "{Ctrl+m}"
				ELSE
				IF _arg1 = "msimn" THEN 
				SendKeys "{Shift+Tab}"
				SendKeys "{Ctrl+n}"
				ELSE
				SendKeys "{Ctrl+n}"
				END IF
				END IF
				END IF
				Wait 500
				SendKeys "" + _arg2 + ""
				Wait 100
				IF _arg1 = "Thunderbird" THEN 
				Wait 500
				SendKeys "{Enter}"
				Wait 500
				SendKeys "{Alt+s}{Tab}"
				ELSE
				IF _arg1 = "Eudora" THEN 
				Wait 100
				SendKeys "{Alt+a}{Tab}"
				ELSE
				IF _arg1 = "msimn" THEN 
				Wait 500
				SendKeys "{Shift+Tab}"
				ELSE
				Wait 500
				SendKeys "{Enter}"
				SendKeys "{Tab 4}"
				SendKeys "{Home}"
				END IF
				END IF
				END IF
				Wait 500
				IF _arg3 = "Dear" THEN SendKeys "Dear " + _arg2 + ", {Enter 2}"
				IF _arg3 = "Hi" THEN SendKeys  "Hi " + _arg2 + ", {Enter 2}"
				IF _arg3 = "Plain" THEN SendKeys  + _arg2 + ", {Enter 2}"
				IF _arg1 = "Thunderbird" THEN 
				SendKeys "{Alt+s}"
				ELSE
				IF _arg1 = "Eudora" THEN 
				SendKeys "{Alt+u}"
				ELSE
				SendKeys "{Shift+Tab}"
				END IF
				END IF
			}
		}
