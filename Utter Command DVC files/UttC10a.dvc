		COMMAND "<1-20> <FilePathFPFBottom>" {
			SCRIPT {
				' SClipFile at 72
				IF _arg2 = "File" XOR _arg2 = "File Bottom" THEN DllCall "UttC.dll", 72, _arg1
				IF _arg2 = "Path" XOR _arg2 = "File Path" THEN SendKeys "C:\Documents and Settings\All Users\Documents\UC Files\SpeechClipboard\" + _arg1 + ".txt"
				IF _arg2 = "File Bottom" THEN 
				Wait 500
				SendKeys "{Ctrl+END}"
				END IF
			}
		}
 
		COMMAND "List <1-20> <FilePathFPBottomFB>" {
			SCRIPT {
				p$ = "List " + _arg1
				' SClipFile at 72
				IF _arg2 = "File" XOR _arg2 = "File Bottom" XOR _arg2 = "Bottom" THEN DllCall "UttC.dll", 72, p$
				IF _arg2 = "Path" XOR _arg2 = "File Path" THEN SendKeys "C:\Documents and Settings\All Users\Documents\UC Files\SpeechClipboard\List " + _arg1 + ".txt"
				IF _arg2 = "File Bottom" XOR _arg2 = "Bottom" THEN 
				Wait 500
				SendKeys "{Ctrl+END}"
				END IF
			}
		}
