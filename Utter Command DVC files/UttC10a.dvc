		COMMAND "<CopyCutWLGCAllC> To <1-20> <FileEnterFEReturnStayFRS>" {
			SCRIPT {
				IF _arg1 = "All Copy" THEN SendKeys "{Ctrl+a}{Ctrl+c}"
				IF _arg1 = "Copy" THEN SendKeys "{Ctrl+c}"
'				IF _arg1 = "Letter Copy" THEN SendKeys "{Shift+Right}{Ctrl+c}"
'				IF _arg1 = "Letter Cut" THEN SendKeys "{Shift+Right}{Ctrl+x}"
				IF _arg1 = "Word Copy" THEN SendKeys "{Ctrl+Left}{Shift+Ctrl+Right}{Ctrl+c}"
'				IF _arg1 = "Word Top Copy" THEN SendKeys "{Shift+Ctrl+Left}{Ctrl+c}"
'				IF _arg1 = "Word Bottom Copy" THEN SendKeys "{Shift+Ctrl+Right}{Ctrl+c}"
				IF _arg1 = "Line Copy" THEN SendKeys "{Home}{Shift+End}{Ctrl+c}"
				IF _arg1 = "Line Top Copy" THEN SendKeys "{Shift+Home}{Ctrl+c}"
				IF _arg1 = "Line Bottom Copy" THEN SendKeys "{Shift+End}{Ctrl+c}"
				IF _arg1 = "Graph Copy" THEN SendKeys "{Down}{Ctrl+Up}{Shift+Ctrl+Down}{Ctrl+c}"
'				IF _arg1 = "Graph Top Copy" THEN SendKeys "{Shift+Ctrl+Up}{Ctrl+c}"
'				IF _arg1 = "Graph Bottom Copy" THEN SendKeys "{Shift+Ctrl+Down}{Ctrl+c}"
				IF _arg1 = "Cut" THEN SendKeys "{Ctrl+x}"
				IF _arg1 = "Word Cut" THEN SendKeys "{Ctrl+Left}{Shift+Ctrl+Right}{Ctrl+x}"
'				IF _arg1 = "Word Top Cut" THEN SendKeys "{Shift+Ctrl+Left}{Ctrl+x}"
'				IF _arg1 = "Word Bottom Cut" THEN SendKeys "{Shift+Ctrl+Right}{Ctrl+x}"
				IF _arg1 = "Line Cut" THEN SendKeys "{Home}{Shift+End}{Ctrl+x}"
				IF _arg1 = "Line Top Cut" THEN SendKeys "{Shift+Home}{Ctrl+x}"
				IF _arg1 = "Line Bottom Cut" THEN SendKeys "{Shift+End}{Ctrl+x}"
				IF _arg1 = "Graph Cut" THEN SendKeys "{Down}{Ctrl+Up}{Shift+Ctrl+Down}{Ctrl+x}"
'				IF _arg1 = "Graph Top Cut" THEN SendKeys "{Shift+Ctrl+Up}{Ctrl+x}"
'				IF _arg1 = "Graph Bottom Cut" THEN SendKeys "{Shift+Ctrl+Down}{Ctrl+x}"
				Wait 10
				
				' param1$ is the target file.
				param1$ = "C:\Documents and Settings\All Users\Documents\UC Files\SpeechClipboard\" + _arg2+".txt"
				' number at the end of the line is the text buffer -- changed from 512 9/27/06 KCP (MD found the bug)
				param4$ = DllCall$( "UttC.dll", "PasteFromClipboard", "", 32768 ) 
				'MsgBoxConfirm param4$, 64, "Wrenz Diagnostics"
				param$ = param1$ + "|" +  param4$
				' jwh - 12/01/05 param$ = param1$ + "|"
				
				' At this point param$ looks something like:
				' "C:\Documents and Settings\All Users\Documents\UC Files\SpeechClipboard\" + _arg2+".txt" + [return value from DllCall; not used!?]
 
				' SetFileData at 79
				DllCall "UttC.dll", 79, param$
 
				' Text-to-Speech utility.
				IF _arg3 = "Stay" XOR _arg3 = "File Stay" XOR _arg3 = "Enter" XOR _arg3 = "File Enter" THEN
				Wait 500
				' SClipFile at 72
				DllCall "UttC.dll", 72, _arg2
				Wait 100
				SendKeys "{Ctrl+End}"
				IF _arg3 = "Enter" XOR _arg3 = "File Enter" THEN SendKeys "{Enter}"
				ELSE 
				TTSPlayString "" + _arg1 + " to " + _arg2 + " File"
				END IF
			}
		}
