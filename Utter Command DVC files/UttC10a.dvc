				'param1$ = full path to text file |
				'param2$ = data to insert in file |
				'param3$ = line to insert data. If the line number is greater than lines of data, the data will be placed on the last line
				param1$ = "C:\Documents and Settings\All Users\Documents\UC Files\SpeechClipboard\" + _arg1 +".txt"
				param2$ = "blah" 
				param4$ = DllCall$( "UttC.dll", "PasteFromClipboard", "", 32768 ) 
				param3$ = "10000"
				'MsgBoxConfirm param1$, 52, param4$
				param$ = param1$ + "|" +  param4$ + "|" + param3$
				' SetFileDataByLine at 80
				DllCall "UttC.dll",80, param$
				wait 50
				TTSPlayString "Add to " + _arg1 + ""
			}
		}
