		COMMAND "Zoom <1-100>"{
			SCRIPT {
				isOpenOffice$ = DllCall$( "UttC.dll", "IsForegroundWindowByTitleRE", "OpenOffice.org", 512 )
				IF (instr(isOpenOffice$,"true")) > 0 THEN
				SendKeys "{Alt+v}"
				SendKeys "z"				
				Wait 100
				SendKeys "v"
				SendKeys "{Home+Shift+End}"
				ELSE
