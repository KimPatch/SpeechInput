#6g. Key combinations several times in a row
 
		COMMAND "<1-40> <AlternateControlUnder> <LeftRightPageUpPD>" {
			SCRIPT {
				IF _arg3 = "Page Up" THEN  _arg3 = "PgUp"
				IF _arg3 = "Page Down" THEN  _arg3 = "PgDn"
				IF _arg2 = "Alternate" XOR _arg2 = "Under" THEN SendSystemKeys "{Alt+" + _arg3 + " " + _arg1 + "}"
				IF _arg2 = "Control" THEN SendSystemKeys "{Ctrl+" + _arg3 + " " + _arg1 + "}"
			}
		}
		LIST "AlternateControlUnder" {
			"Alternate"  "Control"  "Under"
		}	
		LIST "LeftRightPageUpPD" {
			"Left"  "Right"  "Page Up"  "Page Down"
		}	
		COMMAND "Go <BackForward>" {
			SCRIPT {
				IF _arg1 = "Back" THEN SendSystemKeys "{Alt+Left}"
				IF _arg1 = "Forward" THEN SendSystemKeys "{Alt+Right}"
			}
		}
		LIST "BackForward" {
			"Back"  "Forward"
		}
		COMMAND "Tab Back" {
			SCRIPT {
				SendSystemKeys "{Shift+Ctrl+Tab}"
			}
		}
		COMMAND "Tab Forward" {
			SCRIPT {
				SendSystemKeys "{Ctrl+Tab}"
			}
		}
		COMMAND "<1-40> <BackForwardTabBTabF> " {
			SCRIPT {
				IF _arg2 = "Back" THEN SendSystemKeys "{Alt+Left " + _arg1 + "}"
				IF _arg2 = "Tab Back" THEN SendSystemKeys "{Shift+Ctrl+Tab " + _arg1 + "}"
				IF _arg2 = "Forward" THEN SendSystemKeys "{Alt+Right " + _arg1 + "}"
				IF _arg2 = "Tab Forward" THEN SendSystemKeys "{Ctrl+Tab " + _arg1 + "}"
			}
		}
		COMMAND "<1-40> <BackForwardTabBTabF> Wait" {
			SCRIPT {
				loop& = Val(_arg1)
				while ( loop& )
				IF _arg2 = "Back" THEN SendSystemKeys "{Alt+Left}"
				IF _arg2 = "Tab Back" THEN SendSystemKeys "{Shift+Ctrl+Tab}"
				IF _arg2 = "Forward" THEN SendSystemKeys "{Alt+Right}"
				IF _arg2 = "Tab Forward" THEN SendSystemKeys "{Ctrl+Tab}"
				Wait 2000
				loop& = loop& - 1
				wend
			}
		}
		COMMAND "<1-40> <BackForwardTabBTabF> Wait <1-10>" {
			SCRIPT {
				loop& = Val(_arg1)
				while ( loop& )
				IF _arg2 = "Back" THEN SendSystemKeys "{Alt+Left}"
				IF _arg2 = "Tab Back" THEN SendSystemKeys "{Shift+Ctrl+Tab}"
				IF _arg2 = "Forward" THEN SendSystemKeys "{Alt+Right}"
				IF _arg2 = "Tab Forward" THEN SendSystemKeys "{Ctrl+Tab}"
				Wait Val(_arg3)*1000
				loop& = loop& - 1
				wend
			}
		}
		LIST "BackForwardTabBTabF" {
			"Back"  "Forward"  "Tab Back" "Tab Forward"
		}	
		COMMAND "<1-40> <ControlShiftC> Tab"{
