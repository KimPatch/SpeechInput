#6e. Single Keys several times in a row
		COMMAND "<1-40> <KeyBkDelEnterSpaceTabShiftTEscapeClosePgDnPgUp>" {
			SCRIPT {
				IF _arg2 = "Escape" THEN  _arg2 = "Esc"
				IF _arg2 = "Delete" THEN  _arg2 = "Del"
				IF _arg2 = "Shift Tab" THEN  _arg2 = "Shift+Tab"
				IF _arg2 = "Space" THEN  _arg2 = " "
				IF _arg2 = "Close" THEN  _arg2 = "Esc"
				IF _arg2 = "Page Up" THEN  _arg2 = "PgUp"
				IF _arg2 = "Page Down" THEN  _arg2 = "PgDn"
				loop& = Val(_arg1)
				while ( loop& )
				SendKeys "{" + _arg2 + "}"
				loop& = loop& - 1
				wend
			}
		}
		LIST "KeyBkDelEnterSpaceTabShiftTEscapeClosePgDnPgUp" {
 			"Page Up"  "Page Down" 
			"Delete"  "Backspace"  "Space"  "Enter"  "Tab" "Shift Tab"
			"Escape"  "Close"  
		}
		COMMAND "<DULRPgDnUp> Times <1-100>" {
			SCRIPT {
				IF _arg1 = "Page Down" THEN  _arg1 = "PgDn"
				IF _arg1 = "Page Up" THEN  _arg1 = "PgUp"
				IF _arg2 = "One Hundred" THEN  _arg2 = "100"
				IF _arg2 = "Two Hundred" THEN  _arg2 = "200"
				SendKeys "{" + _arg1 + " " + _arg2 + "}"
			}
		}
		LIST "DULRPgDnUp" {
			"Down"  "Up"  "Left"  "Right"  "Page Down"  "Page Up"
		}
		COMMAND "<KeyNoDULRPgDnUpPasteShiftTabEscPrintScreen> Times <1-100>" {
			SCRIPT {
				IF _arg1 = "Shift Tab" THEN  _arg1 = "Shift+Tab"
				IF _arg1 = "Paste" THEN  _arg1 = "Ctrl+v"
				if _arg1 = "Escape" then  _arg1 = "Esc"
				if _arg1 = "Delete" then  _arg1 = "Del"
				loop& = Val(_arg2)
				while ( loop& )
				SendKeys "{" + _arg1 + "}"
				loop& = loop& - 1
				wend
			}
		}
		LIST "KeyNoDULRPgDnUpPasteShiftTabEscPrintScreen" {
			"Delete"  "Backspace"  "Space"  "Enter"  "Tab"
			"End"  "Home"  "Paste"  "Escape"  "Shift Tab"  "Print Screen"
		}
		COMMAND "<Characters> Times <1-100>" {
			SCRIPT {
				IF _arg1 = "}" THEN
				loop& = Val(_arg2)
				while ( loop& )
				SendKeys "}" 
				loop& = loop& - 1
				wend
				ELSE
				IF _arg1 = "Minus" XOR _arg1 = "Hyphen" THEN  _arg1 = "-"										
				IF _arg1 = "Plus" THEN  _arg1 = "+"										
				IF _arg1 = "Equals" THEN  _arg1 = "="										
				IF _arg1 = "Percent" THEN _arg1 = "%"												
				IF _arg1 = "Divided" THEN _arg1 = "/"								
				IF _arg1 = "Star" XOR _arg1 = "Times" THEN  _arg1 = "*"
				IF _arg1 = "Open Angle" XOR _arg1 = "Less" THEN _arg1 = "<"
				IF _arg1 = "Close Angle" XOR _arg1 = "Greater" THEN _arg1 = ">"
				IF _arg1 = "Grave" THEN _arg1 = "`"									
				IF _arg1 = "Ampersand" THEN  _arg1 = "&"							
				IF _arg1 = "Pipe" THEN  _arg1 = "|"							
				IF _arg1 = "Exclamation" THEN  _arg1 = "!"										
				IF _arg1 = "Dollar" THEN  _arg1 = "$"										
				IF _arg1 = "Number" THEN  _arg1 = "#"										
				IF _arg1 = "Question" THEN  _arg1 = "?"										
				IF _arg1 = "Single" THEN  _arg1 = "'"
				SendKeys "{" + _arg1 + " " + _arg2 + "}"
				END IF
			} 
		}
		LIST "Characters" {
            "!"  "@"  "#"  "$"  "%"  "^"  "Ampersand"  "("  ")"
			"~"  "Hyphen"  "Minus"  "_"  "Equals"
			"Plus"  "Pipe"  "Grave" "["  "{"
			"]"  "}"  ";"  ":"  "\""  ","  "."  "/"  "?"
			"Exclamation"  "Dollar"  "Number"  "Question"  "Single" "Times"  "Star"  "Percent"  "Divided"
			"Greater"  "Less"  "Open Angle"  "Close Angle"
# trimmed two word non-ambiguous and unnecessary symbols, added Hyphen
		}
		COMMAND "<Alph0-9> Times <1-100>" {
			SCRIPT {
				
				IF _arg1 = "Zero" THEN  _arg1 = "0"
				loop& = Val(_arg2)
				while ( loop& )
				SendKeys MID$(_arg1,0,1)
				loop& = loop& - 1
				wend
			}
		}
		COMMAND "<CapLetter> <Alph> Times <1-100>" {
			SCRIPT {
				
				loop& = Val(_arg3)
				while ( loop& )
				IF _arg1 = "Cap" THEN SendKeys "{Shift+"+MID$(_arg2,0,1)+"}"
				IF _arg1 = "Letter" THEN SendKeys MID$(_arg2,0,1)
				loop& = loop& - 1
				wend
			}
		}
		LIST "CapLetter" {
			"Cap"  "Letter"
		}
		LIST "Alph" {
#			"a" "b" "c" "d" "e" "f" "g" "h" "i" "j" "k" "l" "m"
#			"n" "o" "p" "q" "r" "s" "t" "u" "v" "w" "x" "y" "z"
			"alpha"  "bravo"  "charlie"  "delta"  "echo"  "foxtrot"  "golf"  "hotel"  "india"  "juliett"  "kilo"  "lima"  "mike"  "november"  "oscar"  "papa"  "quebec"  "romeo"  "sierra"  "tango"  "uniform"  "victor"  "whiskey"  "xray"  "yankee"  "zulu"  
		}
		COMMAND "<NumPad> <NumPadKey,0-9> Times <1-100>" {
			SCRIPT {
				loop& = Val(_arg3)
				while ( loop& )
				if _arg2 = "Point" XOR _arg2 = "Dot" THEN SendSystemKeys"{NumKey.}"
				if _arg2 = "Slash" XOR _arg2 = "Divide" THEN SendSystemKeys"{NumKey/}"
				if _arg2 = "Star" XOR _arg2 = "Times" THEN SendSystemKeys "{NumKey*}"
				if _arg2 = "Minus" THEN SendSystemKeys"{NumKey-}"
				if _arg2 = "Plus" THEN SendSystemKeys"{NumKey+}"
				if _arg2 = "Enter" THEN SendSystemKeys"{NumKeyEnter}"
				if _arg2 = "0" XOR _arg2 = "1" XOR _arg2 = "2" XOR _arg2 = "3" XOR _arg2 = "4" XOR _arg2 = "5" XOR _arg2 = "6" XOR _arg2 = "7" XOR _arg2 = "8" XOR _arg2 = "9" XOR _arg2 = "0" THEN
				SendSystemKeys"{NumKey" + _arg2 + "}"
				END IF
				loop& = loop& - 1
				wend
			}
		}
 
#6f. Basic combinations 
		COMMAND "<1-3AltUnderCtrlSh> <AlphChar0-9Space>" {
			SCRIPT {
				
				IF _arg2 = "Zero" THEN  _arg2 = "0"				
				IF _arg2 = "Minus" THEN  _arg2 = "-"										
				IF _arg2 = "Plus" THEN  _arg2 = "+"										
				IF _arg2 = "Equals" THEN  _arg2 = "="										
				IF _arg2 = "Percent" THEN _arg2 = "%"												
				IF _arg2 = "Divided By" THEN _arg2 = "/"								
				IF _arg2 = "Backslash" THEN _arg2 = "\"								
				IF _arg2 = "Star" XOR _arg2 = "Times" THEN  _arg2 = "*"
				IF _arg2 = "Ampersand" THEN  _arg2 = "&"							
				IF _arg2 = "Pipe" THEN  _arg2 = "|"							
				IF _arg2 = "Grave" THEN  _arg2 = "`"	
				IF _arg2 = "Exclamation" THEN  _arg2 = "!"										
				IF _arg2 = "Dollar" THEN  _arg2 = "$"										
				IF _arg2 = "Number" THEN  _arg2 = "#"										
				IF _arg2 = "Question" THEN  _arg2 = "?"										
				IF _arg2 = "Single" THEN  _arg2 = "'"										
				IF _arg2 = "Quote" THEN  _arg2 = """"										
				IF _arg2 = "Open Angle" XOR _arg2 = "Less Than" THEN _arg2 = "<"
				IF _arg2 = "Close Angle" XOR _arg2 = "Greater Than" THEN _arg2 = ">"												
				_var1 = "{"				
				if Instr(_arg1, "Control") > 0 then _var1 = _var1 + "Ctrl+"
				if Instr(_arg1, "Alternate") XOR Instr (_arg1, "Under") > 0 then _var1 = _var1 + "Alt+"
				if Instr(_arg1, "Shift") > 0 then _var1 = _var1 + "Shift+"
				IF _arg2 = "Space" THEN 
				SendKeys "" + _var1 + "Space}"
				ELSE
				SendKeys _var1 + MID$(_arg2,0,1) +"}"
				END IF
			}
		}
