			}
		}
		COMMAND "<2-4AltUnderCtrlShWin>"{
			SCRIPT {
				IF _arg1 = "Shift Control" THEN SendKeys "{Shift+Ctrl}"
				IF _arg1 = "Shift Alternate" XOR _arg1 = "Shift Under" THEN SendKeys "{Shift+Alt}"
				IF _arg1 = "Control Alternate" THEN SendKeys "{Ctrl+Alt}"
				IF _arg1 = "Shift Control Alternate" THEN SendKeys "{Shift+Ctrl+Alternate}"
				IF _arg1 = "Win Key Shift" THEN SendKeys "{Win+Shift}"
				IF _arg1 = "Win Key Control" THEN SendKeys "{Win+Ctrl}"
				IF _arg1 = "Win Key Alternate" XOR _arg1 = "Win Key Under" THEN SendKeys "{Win+Alt}"
				IF _arg1 = "Win Key Control" THEN SendKeys "{Win+Ctrl}"
				IF _arg1 = "Win Key Shift Control" THEN SendKeys "{Win+Shift+Ctrl}"
				IF _arg1 = "Win Key Shift Alternate" XOR _arg1 = "Win Key Shift Under" THEN SendKeys "{Win+Shift+Alt}"
				IF _arg1 = "Win Key Control Alternate" XOR _arg1 = "Win Key Control Under" THEN SendKeys "{Win+Ctrl+Alt}"
				IF _arg1 = "Win Key Shift Control Alternate" XOR _arg1 = "Win Key Shift Control Under" THEN SendKeys "{Win+Shift+Ctrl+Alt}"				
				}
		}
