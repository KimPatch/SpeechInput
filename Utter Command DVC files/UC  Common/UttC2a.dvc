		COMMAND "<DocumentDocWindowWin> Close Times <1-10>" {
			SCRIPT {
				loop& = Val(_arg2)
				while ( loop& )
				IF _arg1 = "Document" XOR _arg1 = "Doc" THEN SendKeys "{Ctrl+F4}"
				IF _arg1 = "Window" XOR _arg1 = "Win" THEN SendKeys "{Alt+F4}"
				Wait 700
				loop& = loop& - 1
				wend				
			}
		}
