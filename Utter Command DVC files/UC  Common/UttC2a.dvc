		COMMAND "<Programs> <OpenCloseNewMaxMinRestore>" {
			SCRIPT {
				IF _arg1 = "Solitaire" THEN 
				HeardWord "start", "Solitaire"
				ELSE			
				IF _arg1 = "Access" THEN  _arg1 = "MSACCESS"
				IF _arg1 = "Base" THEN  _arg1 = "sbase.exe"
