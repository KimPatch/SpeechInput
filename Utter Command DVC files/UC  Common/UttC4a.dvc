		COMMAND "<TouchTTwice> <1-20> <GLLWDUABUSs>" {
			SCRIPT {
				IF _arg1 = "Touch" THEN ButtonClick
				IF _arg1 = "Touch Twice" THEN ButtonClick 1, 2
				IF _arg3 = "Graph Space Ups" THEN SendKeys "{Ctrl+Up}"
				IF _arg3 = "Befores" THEN
				SendKeys "{Shift+Ctrl+Left " + _arg2 + "}" 
				ELSE
				p$ = _arg2 + "|" + _arg3
				' GLLWDUABUSs at 46
				DllCall "UttC.dll", 46, p$
				END IF 
			}
		}


