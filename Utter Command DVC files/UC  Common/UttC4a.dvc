		COMMAND "<MouseDrag> <1-100> <DULR>" {
			SCRIPT {
				p$ = _arg3 + " " + _arg2
				' MoveArrowByOneDirection at 55
				IF _arg1 = "Mouse" THEN DllCall "UttC.dll", 55, p$
				' DragArrowByOneDirection at 17
