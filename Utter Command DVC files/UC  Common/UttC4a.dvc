		COMMAND "Two <0-100> By <0-100>" {
			SCRIPT {
				IF _arg1 = "1" XOR _arg1 = "2" XOR _arg1 = "3" XOR _arg1 = "4" XOR _arg1 = "5" XOR _arg1 = "6" XOR _arg1 = "7" XOR _arg1 = "8" XOR _arg1 = "9" THEN
